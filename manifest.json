{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Discord",
  "description": "Discord - Chat for Gamers",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "urls": [
      "https://discordapp.com/channels/"
    ],
    "launch": {
      "web_url": "https://discordapp.com/channels/@me/"
    }
  },
  "permissions": [
    "unlimitedStorage", "notifications"
  ]
}
